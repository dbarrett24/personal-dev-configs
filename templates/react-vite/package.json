{
    "name": "react-vite-template",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "concurrently --raw --kill-others-on-fail --names eslint,prettier,tsc 'pnpm:lint-ts' 'pnpm:lint-prettier' 'pnpm:ts-check'",
        "lint-fix": "pnpm lint-ts-fix && pnpm lint-prettier-fix",
        "lint-ts": "eslint src --ext .ts,.tsx",
        "lint-prettier": "prettier --cache --check \"src/**/*.{json,js,jsx,ts,tsx}\"",
        "lint-ts-fix": "eslint src --ext .ts,.tsx --fix",
        "lint-prettier-fix": "prettier --cache --write \"src/**/*.{json,js,jsx,ts,tsx}\"",
        "ts-check": "tsc --noEmit",
        "test": "jest",
        "test-watch": "jest --watch",
        "test-coverage": "jest --coverage"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.1",
        "@tanstack/react-query": "^5.80.6",
        "@yourname/basketball-training-ui": "workspace:*",
        "@yourname/theme-system": "workspace:*",
        "jotai": "^2.10.3",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.52.2",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.3.4",
        "@yourname/eslint-config": "workspace:*",
        "@yourname/jest-config": "workspace:*",
        "@yourname/prettier-config": "workspace:*",
        "@yourname/testing-utils": "workspace:*",
        "@yourname/typescript-config": "workspace:*",
        "autoprefixer": "^10.4.20",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss": "^8.4.49",
        "prettier": "^3.5.3",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.8.2",
        "vite": "^6.0.7"
    },
    "prettier": "@yourname/prettier-config"
}

